{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "monthlyContactLimit": {
      "type": "integer",
      "default": 10
    },
    "usedContacts": {
      "type": "integer",
      "default": 0
    },
    "nextResetDate": {
      "type": "date"
    },
    "isApproved": {
      "type": "boolean"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "profile"
    }
  }
}
